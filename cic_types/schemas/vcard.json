{
    "type": "object",
    "properties": {
        "email": {
            "type": "string",
            "format": "email"
        },
        "family": {
            "type": "string",
            "pattern": "^(.*)$"
        },
        "given": {
            "type": "string",
            "pattern": "^(.*)$"
        },
        "tel": {
            "type": "string",
            "pattern": "^(\\+?[0-9]+)$"
        }
    },
    "required": ["family", "given", "tel"]
}
