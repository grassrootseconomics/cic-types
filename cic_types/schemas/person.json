{
    "type": "object",
    "properties": {
        "date_registered": {
            "type": "number",
            "pattern": "^([0-9]+)$"
        },
        "date_of_birth": {
            "types":  "object",
            "properties": {
                "day": {
                    "type": "number",
                    "pattern": "^(\\d{1,2})$"
                },
                "month": {
                    "type": "number",
                    "pattern": "^(\\d{1,2})$"
                },
                "year": {
                    "type": "number",
                    "pattern": "^(\\d{4})$"
                }
            }
        },
        "gender": {"type": "string"},
        "identities": {
            "type": "object",
            "properties": {
                "type": "object",
                "properties": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            }
        },
        "location": {
            "type": "object",
            "properties": {
                "area_name": {"type": "string"},
                "area_type": {"type": [ "string", "null" ]},
                "latitude": {
                    "type": "number",
                    "pattern": "^(\\-?([0-8]?[0-9](\\.\\d+)?|90(.[0]+)?)\\s?)$"
                },
                "longitude": {
                    "type": "number",
                    "pattern": "^(\\-?([1]?[0-7]?[0-9](\\.\\d+)?|180((.[0]+)?)))$"
                },
                "external": {"type": "object"}
            },
            "required": ["area_name"]
        },
        "products": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "vcard": {
            "type": "string",
            "contentEncoding": "base64",
            "contentMediaType": "text/html"
        }
    },
    "required": ["date_registered", "gender", "identities", "location", "products", "vcard"]
}
